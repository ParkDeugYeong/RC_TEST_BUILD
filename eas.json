{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "hooks": {
    "pre-install": "bash ./scripts/eas-build-pre-install.sh"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "apk": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {}
  }
}
